 "html": "{% if letter_head and not no_letterhead -%}\n    <div class=\"letter-head\">{{ letter_head }}</div>\n    <hr>\n{%- endif %}\n<div align=\"center\"><h4 class=\"text-uppercase\"><b>PURCHASE REQUEST FORM</b></h4></div>\n<br/>\n<div>\n  <div class=\"row section-break\">\n    <div class=\"col-xs-6 column-break\">\n      <div class=\"row\">\n        <div class=\"col-xs-6 text-left\">\n          <label>Request Reference</label>\n        </div>\n        {% if doc.request_for_material %}\n        <div class=\"col-xs-6  text-left value\">\n          <strong>: </strong>{{doc.request_for_material}}\n        </div>\n        {% endif %}\n      </div>\n      <div class=\"row\">\n        {% if doc.project %}\n        <div class=\"col-xs-4 text-left\">\n          <label>Project</label>\n        </div>\n        <div class=\"col-xs-8  text-left value\">\n          <strong>: </strong>{{doc.project}}\n        </div>\n        {%- endif -%}\n      </div>\n      <div class=\"row\">\n        {% if doc.project_details %}\n        <div class=\"col-xs-4 text-left\">\n        </div>\n        <div class=\"col-xs-8  text-left value\">\n          {{doc.project_details}}\n        </div>\n        {%- endif -%}\n      </div>\n      <div class=\"row\">\n        {% if doc.employee %}\n        <div class=\"col-xs-4 text-left\">\n          <label>Employee</label>\n        </div>\n        <div class=\"col-xs-8  text-left value\">\n          <strong>: </strong>{{doc.employee}}\n        </div>\n        {%- endif -%}\n      </div>\n      <div class=\"row\">\n        {% if doc.employee_name %}\n        <div class=\"col-xs-4 text-left\">\n          <label>Employee Name</label>\n        </div>\n        <div class=\"col-xs-8  text-left value\">\n          <strong>: </strong>{{doc.employee_name}}\n        </div>\n        {%- endif -%}\n      </div>\n      <div class=\"row\">\n        {% if doc.department %}\n        <div class=\"col-xs-4 text-left\">\n          <label>Department</label>\n        </div>\n        <div class=\"col-xs-8  text-left value\">\n          <strong>: </strong>{{doc.department}}\n        </div>\n        {%- endif -%}\n      </div>\n    </div>\n\n    <div class=\"col-xs-6 column-break\">\n      <div class=\"row\">\n        <div class=\"col-xs-4 text-left\">\n          <label>Date</label>\n        </div>\n        {% if doc.transaction_date %}\n        <div class=\"col-xs-8  text-left value\">\n          <strong>: </strong>{{frappe.utils.formatdate(doc.transaction_date)}}\n        </div>\n        {% endif %}\n      </div>\n      <div class=\"row\">\n        {% if doc.priority %}\n        <div class=\"col-xs-4 text-left\">\n          <label>Priority</label>\n        </div>\n        <div class=\"col-xs-8  text-left value\">\n          <strong>: </strong>{{doc.priority}}\n        </div>\n        {%- endif -%}\n      </div>\n    </div>\n  </div>\n  <br/>\n  <div class=\"row section-break\">\n    <div class=\"col-xs-12\">\n      {% if doc.technical_remarks %}\n      <label>Technical Verification:</label>\n      <p>\n        {{doc.technical_remarks}}\n      </p>\n      {% endif %}\n    </div>\n  </div>\n  <div class=\"row section-break\">\n    <div class=\"col-xs-12\">\n      {% if doc.notes %}\n      <label>Notes:</label>\n      <p>\n        {{doc.notes}}\n      </p>\n      {% endif %}\n    </div>\n  </div>\n</div>\n\n<table class=\"table\">\n  <tbody>\n    {%- if doc.items -%}\n    <tr>\n      <th>NR</th>\n      <th>Item Name</th>\n      <th class=\"text-left\">Description</th>\n      <th class=\"text-right\">UOM</th>\n      <th>Quantity</th>\n      <th>Required Date</th>\n    </tr>\n\n    {%- for row in doc.items -%}\n    <tr>\n      <td style=\"width: 5%;border:none;\">\n        {{loop.index}}\n      </td>\n      <td style=\"width: 20%;border:none;\">\n        {{ row.item_name }}\n      </td>\n      <td style=\"width: 40%;border:none;\">\n        {{ row.description }}\n      </td>\n      <td style=\"width: 10%;text-align: right;border:none;\">\n  